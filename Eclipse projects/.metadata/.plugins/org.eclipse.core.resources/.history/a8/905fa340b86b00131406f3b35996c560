public class LList
{
	class Node
	{
		public int data;
		Node next;

		public Node(int data)
		{
			this.data=data;
		}
		

	}
	Node root = null;
	
	public int size()
	{
		int count = 0;
		Node tmp = root;
		while(tmp!=null)
		{
			count++;
			tmp = tmp.next;
		}
		return count;
	}

	@Override
	public String toString()
	{
		Node tmp = root;
		String str = "";
		while(tmp!=null)
		{
			str += tmp.data+", ";
			tmp = tmp.next;
		}
		return "LList [root = " + str + "]";
	}

	public void addStart(int data)
	{
		Node tmp = new Node(data);
		tmp.next = root;
		root = tmp;
	}

	public void delStart()
	{
		root = root.next;
	}


	public void clear() 
	{
		root = null;		
	}




}

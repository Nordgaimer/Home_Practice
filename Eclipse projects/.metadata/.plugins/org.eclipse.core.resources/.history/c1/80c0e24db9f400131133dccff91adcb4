package Main;

public class Test1 {

    private int sum = 0;

    public void add(Integer i) {
        sum += i;
    }

    public int getSum() {
        return sum;
    }

    public static void main(String[] args) {

        Test1 test1 = new Test1();
        for (int i = 0; i < 10; i++) {
            test1.add(i);
        }
        System.out.println(test1);
    }
}
package Main;

import java.io.BufferedWriter;
import java.io.File;
import java.io.FileWriter;
import java.util.List;
import java.io.BufferedWriter;
import java.io.File;
import java.io.FileWriter;
import java.io.IOException;
import java.util.List;
import Exceptions.DBSystemException;
import JdbcUtils.DAO_Manager;

public class Main {

	public static void main(String[] args) throws DBSystemException, IOException {
		DAO_Manager dao = new DAO_Manager();
		List<Integer> list = dao.getDocsChIdToList();
		
		for (Integer l: list){
			System.out.println(l);
			
		
	}
		
//		System.out.println(dao.getHeaderInString(100000001));
//		System.out.println(dao.getProductInString(100000001));
//		System.out.println(dao.getPackTypeInString(100000001));
//		System.out.println(dao.getProductSpendsInString(100000001));
//		System.out.println(dao.getPackTypeInString(100000001));
//		
		
		String content = "This is the content to write into file";
		 
		File file = new File("c:\\newfile.xml");

		// if file doesnt exists, then create it
		if (!file.exists()) {
			file.createNewFile();
		}

		FileWriter fw = new FileWriter(file.getAbsoluteFile());
		BufferedWriter bw = new BufferedWriter(fw);
		bw.write(content);
		bw.close();

		System.out.println("Done");
	}

	
}

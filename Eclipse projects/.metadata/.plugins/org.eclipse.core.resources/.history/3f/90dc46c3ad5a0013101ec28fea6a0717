public class HW 
{
	public static void hw_1 ()
	{
		for (int i =0;i<7;i++)
		{
			System.out.println("*******");	
		}
	
		for (int i =0;i<1;i++)
		{
			System.out.println("*******");	
			for (int j=0;j<7;j++)
			{
				System.out.println("*     *");
			}
			System.out.println("*******");	
		}
		
	}
}

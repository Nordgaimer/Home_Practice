package JdbcUtils;

public class XMLDescriptionMaker {

	public static String addDescr (String columnName){
		if (columnName.equals("ChID")) {
			return " Внутренний номер документа";
		}
		if (columnName.equals("DocID")) {
			return " Внешний номер документа";
		}
		if (columnName.equals("DocDate")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		if (columnName.equals("ChID")) {
			return " Сервисный номер документа";
		}
		return "" ;
				
	}
}

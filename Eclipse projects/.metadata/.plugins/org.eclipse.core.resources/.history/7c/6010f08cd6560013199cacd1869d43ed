import javax.swing.JButton;
import javax.swing.JPanel;
import javax.swing.JTextField;


public class WndPanel extends JPanel 
{
	JButton btn = null;
	JTextField txt = null;
	public WndPanel()
	{
		setLayout(null);
		btn = new JButton("My Button");
		add(btn);
		btn.setBounds(10, 10, 120, 22);
		
		txt = new JTextField();
		txt.setBounds(10, 40, 120, 22);
		add(txt);
	}
}
public class Llist
{
	class Node
	{
		int data;
		Node head;
		
		Node(int data,Node head,Node tail)
		{
			this.data=data;
			this.head=head;
			
		}
	}
	Node root = null;

	public void addStart (int data, Node head)
	{	
		Node tmp = new Node(data,head,tail);
		tmp.next=root;
		root=tmp.next;
	}	


}

